{
  "name": "react-rrule-generator-ts",
  "version": "0.3.2",
  "description": "This is ReactJS project based on Create React Library and using Bootstrap styling. It's built with the help of a great rrule.js library.\n It also uses: \n -lodash \n -date-fns \n- @material-ui/pickers",
  "author": "pollylnd",
  "license": "MIT",
  "repository": "pollylnd/react-rrule-generator-ts",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs --no-css-modules",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --no-css-modules",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.12.38",
    "@types/react": "^16.5.2",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^16.5.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.5.2",
    "react-autosize-textarea": "^7.1.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-datetime": "^3.0.4",
    "react-dom": "^16.5.2",
    "react-scripts": "^3.4.1",
    "rrule": "^2.6.6",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "date-fns": "^2.16.1",
    "rrule": "^2.6.6"
  }
}
